# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=

# Telegram Chat ID (the group to gate)
TELEGRAM_CHAT_ID=

# Web server URL (your Vercel deployment URL)
WEB_URL=https://your-project.vercel.app

# HMAC secret for stateless nonce signing (generate a random string)
NONCE_SECRET=

# $CLAWD token on Base (default)
CLAWD_TOKEN_ADDRESS=0x9f86dB9fc6f7c9408e8Fda3Ff8ce4e78ac7a6b07

# Minimum balance to pass gate (in smallest unit, default 10M with 18 decimals)
CLAWD_MIN_BALANCE=10000000000000000000000000

# Base RPC URL
BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/YOUR_KEY

# Bot settings
VERIFICATION_TIMEOUT_MINUTES=10
RECHECK_INTERVAL_HOURS=24
